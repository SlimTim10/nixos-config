    };

  fileSystems."/boot" =
    { device = "/dev/disk/by-uuid/0BF8-CA33";
      fsType = "vfat";
      options = [ "fmask=0022" "dmask=0022" ];
    };

  swapDevices = [ ];

  boot.initrd.luks.devices = {
    crypted = {
      device = "/dev/disk/by-uuid/b399909f-1b9a-4edf-9bdb-06a1feac213c";
      preLVM = true;
    };
